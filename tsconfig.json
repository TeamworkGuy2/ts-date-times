  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [ "es2017" ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
